import { McpServer } from '@modelcontextprotocol/sdk/server/mcp.js';
import { Reflector } from '@nestjs/core';
import { DiscoveryService } from './discovery.service';
import { McpLoggerService } from './logger.service';
import { SessionManager } from './session.manager';
export declare class RegistryService {
    private readonly discoveryService;
    private readonly logger;
    private readonly reflector;
    private readonly sessionManager;
    constructor(discoveryService: DiscoveryService, logger: McpLoggerService, reflector: Reflector, sessionManager: SessionManager);
    registerAll(server: McpServer): Promise<void>;
    private getDecoratorType;
    private getHandlerArgs;
    private runGuards;
    private wrappedHandler;
    private registerResources;
    private registerPrompts;
    private registerTools;
}
